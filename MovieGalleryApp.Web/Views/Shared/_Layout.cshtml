<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewData["Title"]</title>

    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Roboto:wght@100;300;400;500;700;900&family=Sen:wght@400;700;800&display=swap" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css" />

    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/MovieGalleryApp.Web.styles.css" asp-append-version="true" />*@

    <link rel="stylesheet" href="~/plugins/toastr/toastr.css"/>
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/details.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/dropDownButton.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/editMovie.css" asp-append-version="true" />
</head>

<body>
    <!-- Navbar -->
    <partial name="_Navbar" />

    <!-- Sidebar -->
    <partial name="_Sidebar" />

    <!-- Main -->
    <div class="container">
        @RenderBody()
    </div>

    <script src="~/plugins/jquery/jquery.min.js"></script>
    <script src="~/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/plugins/toastr/toastr.min.js"></script>
    <script src="~/js/site.js"></script>
    <script src="~/js/dropDownButton.js"></script>
    <partial name="_UserMessages" />
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>